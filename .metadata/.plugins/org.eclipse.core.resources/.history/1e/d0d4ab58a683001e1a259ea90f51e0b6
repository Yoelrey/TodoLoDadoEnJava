package Metros100;

public class Atleta implements Runnable{

	public Atleta() {}
	
	
	
	
	public Atleta(int dorsal, String pais, String nombre) {
		super();
		//this.tiempo = tiempo;
		this.dorsal = dorsal;
		this.pais = pais;
		this.nombre = nombre;
	}




	double tiempo;
	int dorsal;
	String pais;
	String nombre;
	@Override
	public void run() {
		
		System.out.println(this.nomb+"preparado");
		
	}




	@Override
	public String toString() {
		return "Atleta [tiempo=" + tiempo + ", dorsal=" + dorsal + ", pais=" + pais + ", nombre=" + nombre + "]";
	}

}
